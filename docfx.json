{
  "metadata": [
    {
      "src": [
        {
          "src": "./src",
          "files": [
            "**/*.sln"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "**/*.swagger.json"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "BirdAPI",
      "_appTitle": "BirdAPI",
      "_enableSearch": true,
      "pdf": true
    }
  }
}